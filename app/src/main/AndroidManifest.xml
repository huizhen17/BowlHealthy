<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bowlhealthy">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyCd7Vr5-nw41wge3QdNWdRC-tBCp3y--00"/>

        <activity android:name=".AboutUs"/>
        <activity android:name=".TermCondition" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FAQ" />
        <activity android:name=".SingleHistory" />
        <activity
            android:name=".SingleMenu"
            android:screenOrientation="nosensor" />
        <activity
            android:name=".MenuActivity"
            android:screenOrientation="nosensor"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name=".CheckOut"
            android:screenOrientation="nosensor" />
        <activity
            android:name=".OrderHistory"
            android:screenOrientation="nosensor" />
        <activity
            android:name=".MyCart"
            android:screenOrientation="nosensor" />
        <activity
            android:name=".MyFavourite"
            android:screenOrientation="nosensor" />
        <activity
            android:name=".MyAccount"
            android:screenOrientation="nosensor" />
        <activity
            android:name=".ForgetPassword"
            android:screenOrientation="nosensor" />
        <activity
            android:name=".SignUp"
            android:screenOrientation="nosensor" />
        <activity android:name=".LoginActivity"></activity>
        <activity
            android:name=".MainActivity"
            android:screenOrientation="nosensor" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>